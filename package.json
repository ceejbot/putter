{
  "name": "putter",
  "description": "a fanfic archive that doesn't suck, in node",
  "version": "0.0.0",
  "author": "C J Silverio <ceejceej@gmail.com>",
  "bugs": {
    "url": "https://github.com/ceejbot/putter/issues"
  },
  "config": {
    "blanket": {
      "pattern": [
        "lib"
      ],
      "data-cover-never": [
        "node_modules",
        "test"
      ],
      "output-reporter": "spec",
      "travis-cov": {
        "threshold": 90
      }
    }
  },
  "dependencies": {
    "async": "~1.4.2",
    "bcrypt": "~0.8.5",
    "bhttp": "~1.2.1",
    "bistre": "~1.0.1",
    "blade": "~3.3.0",
    "body-parser": "~1.13.3",
    "bole": "~2.0.0",
    "common-log-string": "~0.2.1",
    "connect-timeout": "~1.7.0",
    "cookie-parser": "~1.3.5",
    "csurf": "~1.8.3",
    "express": "~4.13.3",
    "hiredis": "~0.4.1",
    "js-yaml": "~3.4.0",
    "lodash": "~3.10.1",
    "marked": "~0.3.5",
    "multer": "~1.0.3",
    "node-uuid": "~1.4.3",
    "numbat-collector": "~1.4.1",
    "numbat-emitter": "~2.0.1",
    "polyclay": "~1.7.2",
    "polyclay-redis": "~0.1.0",
    "polyclay-rethink": "~0.1.2",
    "prefix-completer": "~0.2.4",
    "prettydate": "0.0.1",
    "redis": "~0.12.1",
    "restify": "~4.0.0",
    "serve-favicon": "~2.3.0",
    "tom": "0.0.1",
    "toml": "~2.3.0",
    "toml-require": "~1.0.1",
    "travis-cov": "~0.2.5",
    "yargs": "~3.21.0"
  },
  "devDependencies": {
    "blanket": "~1.1.7",
    "jscs": "~2.1.0",
    "jshint": "~2.8.0",
    "mocha": "~2.2.5",
    "mocoverage": "~1.0.0",
    "must": "~0.12.0"
  },
  "homepage": "https://github.com/ceejbot/putter",
  "keywords": [
    "fanfiction",
    "archive",
    "express"
  ],
  "license": "ISC",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ceejbot/putter.git"
  },
  "scripts": {
    "auth": "NODE_ENV=development node bin/run-server auth",
    "completer": "NODE_ENV=development node bin/run-server completer",
    "data": "NODE_ENV=development node bin/run-server data",
    "lint": "jscs lib test bin api-auth api-completer api-data",
    "test": "mocha -t 10000 --check-leaks --ui exports --require blanket -R mocoverage test/test-*.js",
    "test-cov": "mocha -t 10000 --require blanket -R travis-cov test/test-*.js",
    "test-travis": "mocha -R spec -t 10000 test/test-*.js && npm run test-cov"
  }
}
