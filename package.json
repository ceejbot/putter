{
  "name": "putter",
  "description": "a fanfic archive that doesn't suck, in node",
  "version": "0.0.0",
  "author": "C J Silverio <ceejceej@gmail.com>",
  "bugs": {
    "url": "https://github.com/ceejbot/putter/issues"
  },
  "dependencies": {
    "@npm/pg-db-session": "~1.3.2",
    "async": "~2.5.0",
    "aws-sdk": "~2.84.0",
    "axios": "~0.16.2",
    "bcrypt": "~1.0.2",
    "bistre": "~1.0.1",
    "bluebird": "~3.5.0",
    "body-parser": "~1.17.2",
    "bole": "~3.0.2",
    "chalk": "~2.0.1",
    "choo": "~5.6.2",
    "common-log-string": "~0.3.0",
    "connect-flash": "~0.1.1",
    "connect-redis": "~3.3.0",
    "cookie-parser": "~1.4.3",
    "csurf": "~1.9.0",
    "db-migrate": "~0.10.0-beta.21",
    "db-migrate-pg": "~0.2.4",
    "dotenv": "~4.0.0",
    "express": "~4.15.3",
    "express-session": "~1.15.3",
    "express-validator": "~3.2.1",
    "get-form-data": "~1.2.5",
    "helmet": "~3.6.1",
    "joi": "~10.6.0",
    "js-yaml": "~3.9.0",
    "lodash": "~4.17.4",
    "markdown-it": "~8.3.1",
    "moment": "~2.18.1",
    "nsqjs": "~0.8.4",
    "ormnomnom": "~3.0.1",
    "otplib": "~4.0.6",
    "pg": "~6.4.0",
    "prefix-completer": "~1.0.0",
    "prettydate": "0.0.1",
    "pug": "~2.0.0-rc.2",
    "redis": "~2.7.1",
    "serve-favicon": "~2.4.3",
    "take-five": "~1.3.4",
    "ua-parser-js": "~0.7.13",
    "uuid": "~3.1.0",
    "yargs": "~8.0.2"
  },
  "devDependencies": {
    "bankai": "~8.1.1",
    "browserify": "~14.4.0",
    "browserify-hmr": "~0.3.5",
    "coveralls": "~2.13.1",
    "envify": "~4.1.0",
    "eslint-config-ceejbot": "~1.1.0",
    "mocha": "~3.4.2",
    "must": "~0.13.4",
    "nyc": "~11.0.3",
    "pug-cli": "~1.0.0-alpha6",
    "pugify": "~2.0.1",
    "tachyons": "~4.7.4",
    "tachyons-buttons": "~1.1.0",
    "tachyons-cli": "~1.0.11",
    "xo": "~0.18.2"
  },
  "homepage": "https://github.com/ceejbot/putter",
  "keywords": [
    "express",
    "fanfiction",
    "fic archive"
  ],
  "license": "ISC",
  "main": "index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/ceejbot/putter.git"
  },
  "scripts": {
    "db:create": "createdbjs ${DB_NAME:-putter_test}",
    "db:down": "bin/run-migration.sh -- down --count 1000",
    "db:migration": "bin/run-migration.sh",
    "db:up": "bin/run-migration.sh -- up",
    "lint": "xo",
    "report:cov": "nyc report --reporter=text-lcov | coveralls",
    "run:auth": "bin/run-server.js api-auth",
    "run:bankai": "bankai assets/js/index.js -w -p 3004",
    "run:completer": "bin/run-server.js api-completer",
    "run:data": "bin/run-server.js api-data",
    "run:web": "bin/run-server.js website",
    "test": "mocha -t 10000 --check-leaks -R spec test/test-*.js",
    "test:cov": "nyc mocha -t 10000 --check-leaks -R spec test/test-*.js",
    "test:db": "createdbjs putter_test; DB_USER=${PGUSER:-$USER} DB_NAME=putter_test npm run db:up",
    "test:travis": "npm run test:cov && npm run lint"
  },
  "xo": {
    "extends": "eslint-config-ceejbot",
    "ignores": [
      "public/**",
      "lib/controllers/**",
      "assets/**"
    ],
    "rules": {
      "unicorn/prefer-starts-ends-with": 0,
      "prefer-arrow-callback": 0
    }
  }
}
