{
  "name": "putter",
  "description": "a fanfic archive that doesn't suck, in node",
  "version": "0.0.0",
  "author": "C J Silverio <ceejceej@gmail.com>",
  "bugs": {
    "url": "https://github.com/ceejbot/putter/issues"
  },
  "dependencies": {
    "async": "~2.3.0",
    "aws-sdk": "~2.41.0",
    "axios": "~0.16.1",
    "bankai": "~7.1.1",
    "bcrypt": "~1.0.2",
    "bistre": "~1.0.1",
    "bluebird": "~3.5.0",
    "bole": "~3.0.2",
    "chalk": "~1.1.3",
    "choo": "~5.1.5",
    "common-log-string": "~0.3.0",
    "dotenv": "~4.0.0",
    "express": "~4.15.2",
    "js-yaml": "~3.8.3",
    "lodash": "~4.17.4",
    "markdown-it": "~8.3.1",
    "moment": "~2.18.1",
    "nsqjs": "~0.8.4",
    "ormnomnom": "~3.0.1",
    "otplib": "~4.0.6",
    "prefix-completer": "~0.4.0",
    "prettydate": "0.0.1",
    "pug": "~2.0.0-beta11",
    "redis": "~2.7.1",
    "restify": "~ 4.3.0",
    "serve-favicon": "~2.4.2",
    "take-five": "~1.3.3",
    "tiny-json-http": "~5.1.0",
    "toml": "~2.3.2",
    "toml-require": "~1.1.0",
    "uuid": "~3.0.1",
    "yargs": "~7.1.0"
  },
  "devDependencies": {
    "bootstrap": "~3.3.7",
    "coveralls": "~2.13.0",
    "eslint-config-ceejbot": "~1.0.4",
    "less": "~2.7.2",
    "mocha": "~3.2.0",
    "must": "~0.13.4",
    "nyc": "~10.2.0",
    "xo": "~0.18.1"
  },
  "homepage": "https://github.com/ceejbot/putter",
  "keywords": [
    "express",
    "fanfiction",
    "fic archive"
  ],
  "license": "ISC",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/ceejbot/putter.git"
  },
  "scripts": {
    "report:cov": "nyc report --reporter=text-lcov | coveralls",
    "run:auth": "bin/run-server.js api-auth",
    "run:completer": "bin/run-server.js api-completer",
    "run:data": "bin/run-server.js api-data",
    "run:static": "bin/serve-static.js",
    "run:web": "bin/run-server.js website",
    "test": "nyc mocha -t 10000 --check-leaks -R spec test/test-*.js",
    "test:cov": "nyc mocha -t 10000 --check-leaks -R spec test/test-*.js",
    "test:style": "xo",
    "test:travis": "npm run test:cov && npm run test:style"
  },
  "xo": {
    "extends": "eslint-config-ceejbot",
    "rules": {
      "unicorn/prefer-starts-ends-with": 0
    }
  }
}
